{
    "name": "chalam/vend-sdk",
    "description": "An unoffical PHP SDK for Vend POS",
    "keywords": [
        "vend-sdk",
        "sdk",
        "point-of-sale",
        "vend"
    ],
    "homepage": "https://github.com/chalamministries/vend-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Pouter",
            "email": "matthew.poulter@simplesquid.co.za",
            "homepage": "https://simplesquid.co.za",
            "role": "Developer"
        },
        {
            "name": "Steven Gauerke",
            "email": "steven@chalamministries.com",
            "homepage": "https://mychm.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "spatie/data-transfer-object": "^1.8",
        "nesbot/carbon": "^2.23"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2",
        "filp/whoops": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "SimpleSquid\\Vend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSquid\\Vend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
